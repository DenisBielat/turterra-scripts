{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/node",
      "config": {
        "buildCommand": "npm run vercel-build"
      }
    },
    {
      "src": "server.mjs",
      "use": "@vercel/node"
    },
    {
      "src": "turtle-profile/**",
      "use": "@vercel/static"
    },
    {
      "src": "icons/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/supabase/(.*)",
      "dest": "/server.mjs",
      "headers": {
        "Access-Control-Allow-Origin": "https://www.turterra.com",
        "Access-Control-Allow-Methods": "GET, OPTIONS",
        "Access-Control-Allow-Headers": "Content-Type"
      }
    },
    {
      "src": "/webflow/(.*)",
      "dest": "/server.mjs",
      "headers": {
        "Access-Control-Allow-Origin": "https://www.turterra.com",
        "Access-Control-Allow-Methods": "GET, OPTIONS",
        "Access-Control-Allow-Headers": "Content-Type"
      }
    },
    {
      "src": "/cloudinary/(.*)",
      "dest": "/server.mjs",
      "headers": {
        "Access-Control-Allow-Origin": "https://www.turterra.com",
        "Access-Control-Allow-Methods": "GET, OPTIONS",
        "Access-Control-Allow-Headers": "Content-Type"
      }
    },
    {
      "src": "/icons/(.*)",
      "dest": "/icons/$1",
      "headers": {
        "Access-Control-Allow-Origin": "https://www.turterra.com",
        "Access-Control-Allow-Methods": "GET, OPTIONS",
        "Access-Control-Allow-Headers": "Content-Type"
      }
    },
    {
      "src": "/turtle-profile/(.*)",
      "dest": "/turtle-profile/$1",
      "headers": {
        "Access-Control-Allow-Origin": "https://www.turterra.com",
        "Access-Control-Allow-Methods": "GET, OPTIONS",
        "Access-Control-Allow-Headers": "Content-Type"
      }
    }
  ]
}
