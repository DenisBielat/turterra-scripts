{
  "version": 2,
  "builds": [
    {
      "src": "server.mjs",
      "use": "@vercel/node"
    },
    {
      "src": "turtle-profile/**",
      "use": "@vercel/static"
    },
    {
      "src": "icons/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/webflow/(.*)",
      "dest": "/server.mjs"
    },
    {
      "src": "/cloudinary/(.*)",
      "dest": "/server.mjs"
    },
    {
      "src": "/physical-feature-descriptions/(.*)",
      "dest": "/server.mjs"
    },
    {
      "src": "/turtle-physical-feature-attributes/(.*)",
      "dest": "/server.mjs"
    },
    {
      "src": "/icons/(.*)",
      "dest": "/icons/$1"
    },
    {
      "src": "/turtle-profile/(.*)",
      "dest": "/turtle-profile/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/turtle-profile/$1"
    }
  ],
  "buildCommand": "npm run build"
}
