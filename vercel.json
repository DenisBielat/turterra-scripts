{
  "version": 2,
  "builds": [
    {
      "src": "server.mjs",
      "use": "@vercel/node"
    },
    {
      "src": "public/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/webflow/(.*)",
      "dest": "/server.mjs"
    },
    {
      "src": "/cloudinary/(.*)",
      "dest": "/server.mjs"
    },
    {
      "src": "/physical-feature-descriptions/(.*)",
      "dest": "/server.mjs"
    },
    {
      "src": "/turtle-physical-feature-attributes/(.*)",
      "dest": "/server.mjs"
    },
    {
      "src": "/(.*)",
      "dest": "/public/$1"
    }
  ]
}
