{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/node",
      "config": {
        "buildCommand": "npm run vercel-build"
      }
    },
    {
      "src": "server.mjs",
      "use": "@vercel/node"
    },
    {
      "src": "turtle-profile/**",
      "use": "@vercel/static"
    },
    {
      "src": "icons/**",
      "use": "@vercel/static"
    }
  ],
  "headers": [
    {
      "source": "/icons/(.*)",
      "headers": [
        {
          "key": "Access-Control-Allow-Origin",
          "value": "https://www.turterra.com"
        },
        {
          "key": "Access-Control-Allow-Methods",
          "value": "GET, OPTIONS"
        },
        {
          "key": "Access-Control-Allow-Headers",
          "value": "Content-Type"
        }
      ]
    }
  ],
  "routes": [
    {
      "src": "/webflow/(.*)",
      "dest": "/server.mjs"
    },
    {
      "src": "/cloudinary/(.*)",
      "dest": "/server.mjs"
    },
    {
      "src": "/physical-feature-descriptions/(.*)",
      "dest": "/server.mjs"
    },
    {
      "src": "/turtle-physical-feature-attributes/(.*)",
      "dest": "/server.mjs"
    },
    {
      "src": "/icons/(.*)",
      "dest": "/icons/$1"
    },
    {
      "src": "/turtle-profile/(.*)",
      "dest": "/turtle-profile/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/turtle-profile/$1"
    }
  ]
}
